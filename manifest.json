{
  "description": "Close the Zoom tab once it's redirected to Zoom",
  "homepage_url": "https://github.com/grrttedwards/auto-close-zoom-tab",
  "manifest_version": 2,
  "name": "Zoom tab closer",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "icon-128.png"
  },
}
